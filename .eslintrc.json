{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "space-before-function-paren": ["error", "never"],
        "indent": ["error", 4]
    }
}
